{
  "cli": {
    "version": ">= 0.56.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_ENV": "Development"
      }
    },
    "preview_staging": {
      "releaseChannel": "staging",
      "distribution": "internal",
      "env": {
        "APP_ENV": "Staging"
      }
    },
    "staging": {
      "releaseChannel": "staging",
      "env": {
        "APP_ENV": "Staging"
      }
    },
    "preview_prod": {
      "ios": {
        "simulator": true
      },
      "distribution": "internal",
      "releaseChannel": "production",
      "env": {}
    },
    "production": {
      "releaseChannel": "production",
      "env": {}
    }
  },
  "submit": {
    "production": {}
  }
}
